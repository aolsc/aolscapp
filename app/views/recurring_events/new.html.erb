<%= javascript_include_tag :defaults %>
<%= calendar_date_select_includes "silver" %>
<h1>Weekly recurring Events for Course '<%=  @course.displayName %>'</h1>

<% form_tag :action => 'create',:course_id => params[:id] do -%>
 <p>
    <label for ="start_date"> Start Date </label><br />
    <%= calendar_date_select_tag "start_date", @start_date, :year_range => 1.months.ago..0.years.ago, :time => true %>
  </p>
  <p>
    <%= submit_tag 'Submit' %>
  </p>

<% end -%>
<%#= link_to 'Back', recurring_events_path %>